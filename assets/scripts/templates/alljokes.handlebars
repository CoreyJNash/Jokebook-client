{{#each jokes as |joke|}}
<div class="show-wrapper">
<h3 class="jokebook"><b><u>{{joke.title}}</b></u></h3>
<h3> JOKE: {{joke.joke}}</h3>
<h3>WRITER: {{joke.writer}}</h3>
<h3>JOKE ID: {{joke.id}}</h3>

  <ul data-id="{{joke.id}}"> 
    <li>{{#if joke.title}}
      {{joke.title}}
      {{else}}
      No joke
      {{/if}}
    </li>
    <li><button class="delete"  type="submit">DELETE</button></li>
  </ul>
</div>


{{/each}}